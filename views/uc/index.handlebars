<a href="/unidadesconsumidoras/cadastrar"><button class="btn btn-success">Nova Unidade Consumidora</button></a>
<hr>

<h2> Lista de Unidades Consumidoras: </h2>
{{#each ucs}}
    <div class="card mt-4">
        <div class="card-body">
        
        <h2>ID da Unidade Consumidora: {{idunidadeconsumidora}}</h2>
        <h3>ID do Cliente: {{idcliente}}</h3>
       
        <p>Cidade: {{cidade}}</p>
        <p>Estado: {{estado}}</p>
        <p>Bairro: {{bairro}}</p>
        <p>Lugradouro: {{lugradouro}}</p>
        <p>Numero: {{numero}}</p>
        <p>Complemento: {{complemento}}</p>
        <p>Instalação: {{instalacao}}</p>
        <p>Data de Instalação: {{datainstalacao}}</p>

        </div>
    </div>
{{else}}
    <h4> Nenhuma Unidade Consumidora registrado</h4>
{{/each}}

<hr>
<a href="/unidadesconsumidoras/cadastrar"><button class="btn btn-success">Nova Unidade Consumidora</button></a>